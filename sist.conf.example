#
#  POSIX Extended Regular Expression Syntax
#  Only lines beginning with '#' are comments.
#  The maximum length of one line is 2048 characters.
#  Blanks can not be colored.
#
#  [NAME].color = [COLOR]
#  [NAME].regex = [REGEX]
#
#  (e.g.)
#  DEFINE1.color = RED
#  DEFINE1.regex = ^(home|fuga)$
#
#  If the color length is 6 => 24bit color (000000~FFFFFF)
#  If the color length is 3 =>  8bit color (000~255)
#
#  Color example
#   * RED
#   * 001
#   * FF0000
#   * \e[31m
#   * \033[31m
#   * \x1b[31m
#   * \033[38;5;1m
#   * \033[38;2;255;0;0m
#   * \033[38;2;255;0;0;48;2;255;255;255;4m
#
#  Predefined colors
#   * BLACK
#   * RED
#   * GREEN
#   * YELLOW
#   * BLUE
#   * MAGENTA
#   * CYAN
#   * WHITE
# 

VENDORS.color = \033[38;5;14m
VENDORS.regex = ^(cisco|aruba|juniper|huawei|arista|riverbed|netscout|yamaha|mellanox)$

COND.color = 001
COND.regex = ^(match|eq|neq|gt|lt|ge|le|range)$

KEYWORD.color = FF0000
KEYWORD.regex = ^(giants|speed|duplex|negotiation|delay|bandwidth|preempt|priority|timers|username|user|license|host|system|systems|address|network|route|routers|neighbor|neighbors|redistribute|default[-]gateway|community|version|class|name|minimum|maximum|level|size|established|source|destination|allowed|timeout|threshold|frequency|keepalive|average|weights|mtu|tunnel|privilege|secret)$

PROTOCOL.color = \033[31m
PROTOCOL.regex = ^(ipv4|ipv6|tcp|udp|icmp|echo|dhcp|domain|nameserver|ssh|telnet|ntp|snmp|snmptrap|syslog|smtp|pop2|pop3|klogin|kshell|rlogin|sunrpc|mpls|rip|isis|ospf|ospfv3|eigrp|bgp|hsrp|vrrp|ipsla|isdn|dial|dialog|hdlc|frame[-]relay|atm|igmp|multicast|broadcast|rsa|pki|isakmp|ipsec|ike|esp|gre|vpn|mvpn|pppoe|qos|cef|pim|ahp|tacacs|cdp|lldp|vtp|spanning[-]tree|lacp|dot1q|l2tun|ethernet|aaa|aaa[-]server)$

CONFIRM.color = RED
CONFIRM.regex = ^(y|yes)$

COMMAND.color = WHITE
COMMAND.regex = ^(exit|end|configure|interface|show|line|copy|username|hostname|password|login|service|ip|crypt|transport|clock|ntp|logging|snmp[-]server|vtp|vlan|name|switch|switchport|router|channel[-]group|port[-]channel|spanning[-]tree|instance|revision|mac|storm[-]control|cdp|lldp|version|offset[-]list|auto[-]summary|auto[-]cost|area|summary[-]address|distribute[-]list|redistribute|default[-]information|passive[-]interface|vrrp|standby|access[-](list|class)|reload|monitor|mls)$

POSITIVE.color = \033[38;5;208m
POSITIVE.regex = ^([[]?up[]]?|enable|enabled|active)$

STRING.color = \033[38;2;230;219;116m
STRING.regex = ^(\".*\"|'.*')$

EMPHASIS.color = \033[38;5;9m
EMPHASIS.regex = ^(no|not|[[]?confirm[]]?|warning|warnings|failed|failures|error|errors|crash)$

INTERFACE.color = CYAN
INTERFACE.regex = ^((Tengigabit|Gigabit|Fast|)Ethernet|(Fa|Gi))$
# INTERFACE.regex = ^((Tengigabit|Gigabit|Fast|)Ethernet[0-9]/[0-9]+|(Fa|Gi)[0-9]/[0-9]?[0-9])$

ACTION.color = \033[38;5;13;4m
ACTION.regex = ^(disable|disabled|deny|shutdown|[[]?down[]]?|[[]?administratively[]]?|none)$

VAR.color = \033[38;5;40m
VAR.regex = ^(trunk|access|full[-]duplex|full|auto[-](duplex|speed)|auto|monitor|any|disable|pvst|mst|rapid[-]pvst|transparent|server|client)$

IPV4_NET.color = RED
IPV4_NET.regex = ^(2[0-4][0-9]|1[0-9]{2}|[1-9][0-9]|[1-8])[.]((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[1-9]?[0-9])[.]){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[1-9]?[0-9])$

IPV4_SUB.color = YELLOW
IPV4_SUB.regex = ^((25[0-5]|24[89])[.])((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[1-9]?[0-9])[.]){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[1-9]?[0-9])$

IPV4_WILD.color = MAGENTA
IPV4_WILD.regex = ^(0[.])((25[0-5]|2[0-4][0-9]|1[0-9]{2}|[1-9]?[0-9])[.]){2}(25[0-5]|2[0-4][0-9]|1[0-9]{2}|[1-9]?[0-9])$

IPV6.color = GREEN
IPV6.regex = ^((([0-9A-Fa-f]{1,4}:){7}(:|([0-9A-Fa-f]{1,4})))|(([0-9A-Fa-f]{1,4}:){6}(:|((25[0-5]|2[0-4][0-9]|[01]?[0-9]{1,2})([.](25[0-5]|2[0-4][0-9]|[01]?[0-9]{1,2})){3})|(:[0-9A-Fa-f]{1,4})))|(([0-9A-Fa-f]{1,4}:){5}((:((25[0-5]|2[0-4][0-9]|[01]?[0-9]{1,2})([.](25[0-5]|2[0-4][0-9]|[01]?[0-9]{1,2})){3})?)|((:[0-9A-Fa-f]{1,4}){1,2})))|(([0-9A-Fa-f]{1,4}:){4}(:[0-9A-Fa-f]{1,4}){0,1}(:((25[0-5]|2[0-4][0-9]|[01]?[0-9]{1,2})([.](25[0-5]|2[0-4][0-9]|[01]?[0-9]{1,2})){3})?))|(([0-9A-Fa-f]{1,4}:){4}(:[0-9A-Fa-f]{1,4}){0,1}((:[0-9A-Fa-f]{1,4}){1,2}))|(([0-9A-Fa-f]{1,4}:){3}(:[0-9A-Fa-f]{1,4}){0,2}(:((25[0-5]|2[0-4][0-9]|[01]?[0-9]{1,2})([.](25[0-5]|2[0-4][0-9]|[01]?[0-9]{1,2})){3})?))|(([0-9A-Fa-f]{1,4}:){3}(:[0-9A-Fa-f]{1,4}){0,2}((:[0-9A-Fa-f]{1,4}){1,2}))|(([0-9A-Fa-f]{1,4}:){2}(:[0-9A-Fa-f]{1,4}){0,3}(:((25[0-5]|2[0-4][0-9]|[01]?[0-9]{1,2})([.](25[0-5]|2[0-4][0-9]|[01]?[0-9]{1,2})){3})?))|(([0-9A-Fa-f]{1,4}:){2}(:[0-9A-Fa-f]{1,4}){0,3}((:[0-9A-Fa-f]{1,4}){1,2}))|(([0-9A-Fa-f]{1,4}:)(:[0-9A-Fa-f]{1,4}){0,4}(:((25[0-5]|2[0-4][0-9]|[01]?[0-9]{1,2})([.](25[0-5]|2[0-4][0-9]|[01]?[0-9]{1,2})){3})?))|(([0-9A-Fa-f]{1,4}:)(:[0-9A-Fa-f]{1,4}){0,4}((:[0-9A-Fa-f]{1,4}){1,2}))|(:(:[0-9A-Fa-f]{1,4}){0,5}((:((25[0-5]|2[0-4][0-9]|[01]?[0-9]{1,2})([.](25[0-5]|2[0-4][0-9]|[01]?[0-9]{1,2})){3})?)|((:[0-9A-Fa-f]{1,4}){1,2}))))$
